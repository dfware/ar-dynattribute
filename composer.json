{
    "name": "dfware/ar-dynattribute",
    "description": "Provides dynamic ActiveRecord attributes stored into the single field in serialized state",
    "keywords": ["yii2", "active", "record", "dynamic", "attribute"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/dfware/ar-dynattribute/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/dfware/ar-dynattribute/wiki",
        "source": "https://github.com/dfware/ar-dynattribute"
    },
    "authors": [
        {
            "name": "Pavel Karelin",
            "email": "devpastet@yandex.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.14"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"dfware\\ar\\dynattribute\\": "src"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
